<h1>Task info</h1>

<h2><%= @task.title %></h2>
<p>Description: <%= @task.details %></p>
<p>Completed: <%= @task.completed %></p>

<%= link_to 'Delete', task_path(@task), data: {turbo_method: :delete, turbo_confirm: "Are u sure?"} %>
<%= link_to 'Edit', edit_task_path(@task) %>
<%= link_to 'Back', tasks_path %>
